    "eslint-plugin-react": "7.4.0",
    "file-loader": "2.0.0",
    "fs-extra": "3.0.1",
    "jest": "20.0.4",
    "moment": "^2.18.1",
    "node-sass": "^4.9.3",
    "normalize.css": "^8.0.0",
    "object-assign": "4.1.1",
    "payment": "^2.3.0",
